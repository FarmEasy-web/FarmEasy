<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FarmEasy</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" />*@
    <link rel="icon" type="imges/jpg" sizes="56x56" href="~/images/logo/fab-icon.jpg">

    <!-- Main style sheet -->
    <link rel="stylesheet" type="text/css" href="~/css/custom.css">
    <!-- responsive style sheet -->
    <link rel="stylesheet" type="text/css" href="~/css/responsive.css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://kit.fontawesome.com/644b872e85.js" crossorigin="anonymous"></script>
    <style>
              
        html,
        body {
            height: 100%;
        }
        .main-container .theme-main-menu .navbar-nav>li>a{
            padding:30px 0;
        }
       
        /* Wrapper for page content to push down footer */
        .container {
            min-height: 85%;
            height: auto;
            /* Negative indent footer by it's height */
            margin: 0 auto -20px;
            margin:0 100px;
        }

        #push,
        #footer {
            /*height: 60px;*/
            padding: 0;
        }

        #footer {
            background-color: #f5f5f5;
            margin-top:100px;
        }
        
        .main-container .theme-main-menu .logo>a:hover
        {
            
            color:#19c40c;
        }
        .main-container .logo
        {
            margin:0px;
          
          padding: 30px 0;
        }
        .social-icon
        {
             /*display:flex;
        justify-content:center;*/
        align-items :flex-end;
        }
        </style>
</head>
<body>
    <header>
        <div class="container">
            <div class="address-list float_right">
                <partial name="_LoginPartial" />
            </div>
        </div>
        <div class="main-container">
            <div class="container">
                <div class="theme-main-menu">
                    <div class="logo float_left">
                        @*    <a asp-controller="Home" asp-action="Index"><img src="~/images/logo/logo.png" alt="logo" class="img-responsive"></a>*@
                        <a asp-controller="Home" asp-action="Index">FarmEasy</a>
                    </div> <!-- End of .logo -->

                    <nav class="navbar navbar-default float_right">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse-1" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>

                        <div class="collapse navbar-collapse float_left" id="navbar-collapse-1">

                            <ul class="nav navbar-nav">
                                <li class="dropdown-holder">
                                    <a asp-controller="Home" asp-action="Index">Home</a>
                                </li>
                                <li class="dropdown-holder">
                                    <a asp-controller="ContactUs" asp-action="Create">Contact Us</a>
                                </li>
                                @if (User.IsInRole("Admin"))
                                {
                                    <li class="dropdown-holder">
                                        <a asp-controller="ManageUser" asp-action="Index">Manage User</a>
                                    </li>
                                }
                            </ul>


                        </div>
                        @*<div class="search_option float_right">
                            <button class="search tran3s dropdown-toggle" id="searchDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-search" aria-hidden="true"></i></button>
                            <form action="#" class="p_color_bg dropdown-menu" aria-labellea:hoverdby="searchDropdown">
                                <input type="text" placeholder="Search here.....">
                                <button><i class="fa fa-search" aria-hidden="true"></i></button>
                            </form>
                        </div>  End of .search_option*@
                        <div class="clear_fix"></div>
                    </nav>
                    <div class="clear_fix"></div>
                </div>
            </div>
        </div>
    </header>






